    "# 导入相关库\n",
    "from bokeh.plotting import figure, output_notebook, show \n",
    "# 准备数据\n",
    "x = [1, 2, 3, 4, 5]\n",
    "y = [6, 7, 2, 4, 5]\n",
    "\n",
    "# 在notbook中展示\n",
    "output_notebook()\n",
    "\n",
    "# 创建一个带有标题和轴标签的新图表,即bokeh类型的图，无需转换!\n",
    "p = figure(title=\"simple line example\", x_axis_label='x', y_axis_label='y')\n",
    "\n",
    "# 添加带有图例和线条粗细的线图渲染器\n",
    "# \n",
    "p.line(x, y, legend_label=\"Temp.\", line_width=2)\n",
    "\n",
    "# 显示图表\n",
    "show(p)"
   ]
